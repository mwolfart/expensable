{
  "name": "expensable-be19",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "tailwindcss -m -i ./styles/globals.css -o app/styles/tailwind.css",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "tailwindcss -w -i ./styles/globals.css -o app/styles/tailwind.css",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "postinstall": "prisma generate"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^5.1.0",
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/serve": "^1.19.1",
    "@remix-run/server-runtime": "^1.19.1",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "bson": "^6.2.0",
    "classnames": "^2.3.2",
    "daisyui": "^3.5.0",
    "firebase": "^10.7.0",
    "firebase-admin": "^11.11.1",
    "formik": "^2.4.5",
    "isbot": "^3.6.13",
    "jsonwebtoken": "^9.0.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-select": "^5.7.4",
    "react-spinners": "^0.13.8",
    "recharts": "^2.9.2",
    "remix-typedjson": "^0.1.7",
    "tiny-invariant": "^1.3.1",
    "use-intl": "^2.19.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.44.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.6",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/react-tag-input": "^6.6.1",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-c8": "^0.27.2",
    "autoprefixer": "^10.4.14",
    "binode": "^1.0.5",
    "c8": "^8.0.1",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^5.0.0",
    "happy-dom": "^10.6.3",
    "msw": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prisma": "^5.1.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
