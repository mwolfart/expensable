{
  "name": "expensable-be19",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:css": "tailwindcss -m -i ./styles/globals.css -o app/styles/tailwind.css",
    "build:remix": "remix build",
    "dev": "remix dev --manual",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate",
    "start": "remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck",
    "postinstall": "prisma generate"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@remix-run/node": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "@remix-run/server-runtime": "^2.3.1",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bson": "^6.2.0",
    "classnames": "^2.3.2",
    "daisyui": "^4.4.19",
    "firebase": "^10.7.0",
    "firebase-admin": "^11.11.1",
    "formik": "^2.4.5",
    "isbot": "^3.7.1",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-select": "^5.8.0",
    "react-spinners": "^0.13.8",
    "recharts": "^2.10.3",
    "tiny-invariant": "^1.3.1",
    "use-intl": "^3.2.2",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^8.44.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.17",
    "@types/react-tag-input": "^6.6.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-c8": "^0.27.2",
    "autoprefixer": "^10.4.16",
    "binode": "^1.0.5",
    "c8": "^8.0.1",
    "cookie": "^0.6.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-prettier": "^5.0.1",
    "happy-dom": "^12.10.3",
    "msw": "^2.0.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "prettier": "3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.0.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
